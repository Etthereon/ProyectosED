@page
@model Frontend.Pages.CreateModel
@{
    ViewData["title"]="Create";
}
<h1>Crear Municipio</h1>
<h3 class="text-danger">@ViewData["Mensaje"]</h3>

<div class="form-horizontal">
    <hr/>
    <div class="row">
        <div class=col-md-4>
            <form method="POST">
                <div asp-validation-summary="ModelOnly" class="text-danger" ></div>
                <div class="form-group">
                    <label asp-for="Municipio.Nombre" class="control-label"></label>
                    <input asp-for="Municipio.Nombre" class="form-control" />
                    <span asp-validation-for="Municipio.Nombre" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <br>
                    <input type="submit" value="Crear" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
    <div>
        <a asp-page="MunIndex">Ir a Inicio</a>
    </div>
</div>
@section Scripts
{
    @{
       await Html.RenderPartialAsync("_ValidationScriptsPartial"); 
    }
}